#!/bin/bash

rm fail_combine.log
for try in $(seq 1 10)
do
    ../bin/internal/combine_events 2> fail_combine.log
    if grep -F "combine_events: No such file or directory" fail.log
    then
        sleep 1
        echo $try >> fail.log
    else
        echo "suceed at $try" >> fail.log
        break
    fi
done
rm -f scratch
